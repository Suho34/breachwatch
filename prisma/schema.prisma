generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DIRECT_URL")
}

model User{
  id Int @id @default(autoincrement())
  email String @unique
  name String
  createdAt DateTime @default(now()) 
  updatedAt DateTime @updatedAt 
}